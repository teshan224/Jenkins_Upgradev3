pipeline{
      agent any
      stages{
            stage('Init'){
                  steps{
                        echo 'Hi. im chamidu Teshan'
                        echo 'we are going to start testing'
                  }
            }
            stage('Build'){
                  steps{
                        echo 'building dample maven projet'
                  }
            }
            stage('Deploy'){
                  steps{
                        echo 'deploy in staging env'
                  }
            }
            stage('Deploy in production'){
                  steps{
                        echo 'deploy in prod env'
                  }
            }


            
      }
}

